<%= form_for(@industry) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
    <br>
    <%= f.label :ind_a, "業種-大分類"%>
    <%= f.text_field :ind_a, class: 'form-control' %>
    <br>
    <%= f.label :ind_b, "業種-中分類" %>
    <%= f.text_field :ind_b, class: 'form-control' %>
    <br>
    <%= f.submit "登録", class: "btn btn-large btn-primary" %>
<% end %>
